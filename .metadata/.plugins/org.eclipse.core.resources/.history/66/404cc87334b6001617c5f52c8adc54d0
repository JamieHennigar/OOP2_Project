
public class Donald extends Character
{
	private int score;
	private int moveX;
	private int moveY;
	
	//no argument constructor
	public Donald()
	{
		score = 0;
		moveX = 0;
		moveY = 0;
	}
	
	//  multiple argument constructor
	public Donald(int score, int moveX, int moveY)
	{
		this.score = score;
		this.moveX = moveX;
		this.moveY = moveY;
	}
	
	//mutators
	public void setScore(int score)
	{
		this.score = score;
	}
	
	public void setMoveX(int moveX)
	{
		this.moveX = moveX;
	}
	
	public void setMoveY(int moveY)
	{
		this.moveY = moveY;
	}
	
	//accessors
	public int getScore()
	{
		return score;
	}
	
	public int getMoveX()
	{
		return moveX;
	}
	
	public int getMoveY()
	{
		return moveY;
	}
}

import javax.swing.*;
import java.awt.*;




public class Character 
{
	private String name;
	private Image mexicanImage;
	private Image donaldImage;
	
	public void setName(String name)
	{
		this.name = name;
	}
	
	public String getName()
	{
		return name;
	}
	public Image donaldImage()
	{
		//JLabel Donald = new JLabel(new ImageIcon("Donald Trump.jpg"));
		ImageIcon donald = new ImageIcon("Donald.png");
		
		return donaldImage;
	}
	
	public Image mexicanImage()
	{
		ImageIcon mexican = new ImageIcon("Mexican.png");
		return mexicanImage;
	}
}



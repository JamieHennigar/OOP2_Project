import java.awt.*;


public class Wall extends Character
{
	
	
	

	public Wall(int topLeftXPosition, int topLeftYPosition, Image image) 
	{
		super(topLeftXPosition, topLeftYPosition, image);
	}

	public void draw(Graphics gRef)
	{
		gRef.setColor(Color.blue);
		gRef.fillRect(topLeftXPosition, topLeftXPosition, width, height);
	}
}

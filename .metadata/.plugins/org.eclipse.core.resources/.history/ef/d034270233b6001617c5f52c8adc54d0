
public class Donald extends Character
{
	private int score;
	
	//no argument constructor
	public Donald()
	{
		score = 0;
	}
	
	//  multiple argument constructor
	public Donald(int score)
	{
		this.score = score;
	}
	
	//mutators
	public void setScore(int score)
	{
		this.score = score;
	}
	
	//accessors
	public int getScore()
	{
		return score;
	}
	
}
